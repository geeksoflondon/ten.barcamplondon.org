    <li class="sponsor"><%= link_to image_tag("sponsors/#{sponsor.last['name'].downcase}.png", title: "#{sponsor.last['name']}"), sponsor.last['link'] %></li>
  <% end %>
<% end %>
   </ul>
</section></div>
